<h3>タグを編集</h3>

<%= form_with(model: tag, local: false) do |form| %>
  <div id="error_messages_for_update">
    <%= render 'layouts/error_messages', model: form.object %>
  </div>
  <div class="field" >
    <%= form.label :name %>
    <%= form.text_field :name, placeholder: 'タグを入力', id: "edit_tag_test"%>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
